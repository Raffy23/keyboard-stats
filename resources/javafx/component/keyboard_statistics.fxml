<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

  <Label layoutX="12.0" layoutY="97.0" text="%subheader.statistics" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
